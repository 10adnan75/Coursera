public class Main
{
    public static int countInversionPairs(int[] a) {
        int size = a.length;
        int count = 0;
        for (int i=0; i<size; i++) {
            for (int j=i+1; j<size; j++) {
                if (a[i]>a[j]) count++;
            }
        }
        return count;
    }
    
	public static void main(String[] args) {
		int[] myArray = {4,5,6,1,2,3};
		System.out.println("Pairs: " + countInversionPairs(myArray));
	}
}
